/ {
	chosen {
		zephyr,console = &cdc_acm_uart0;
	};
	aliases {
		smd-uart = &uart0;
	};

};

&zephyr_udc0 {
	cdc_acm_uart0: cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
	};
};


&uart0_sleep {
	group1 {
		psels = <NRF_PSEL(UART_RX, 0, 25)>, <NRF_PSEL(UART_TX, 0, 24)>;
	};
};

&uart0_default {
	group1 {
		psels = <NRF_PSEL(UART_RX, 0, 25)>, <NRF_PSEL(UART_TX, 0, 24)>;
	};
};

&uart0 {
	compatible = "nordic,nrf-uart"; 
	status = "okay";
	current-speed = <9600>;
	
	argossmd{
		compatible = "arribada,argossmd";
	};
};
