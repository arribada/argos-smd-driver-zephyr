/ {
    chosen {
	   	zephyr,console = &cdc_acm_uart;
		/delete-property/ zephyr,shell-uart;
		/delete-property/ zephyr,uart-mcumgr;
		/delete-property/ zephyr,bt-mon-uart;
		/delete-property/ zephyr,bt-c2h-uart;
    };
};


&uart0 {
    status = "okay";
    current-speed = <9600>;

    argossmd {
        compatible = "arribada,argossmd";
        /* Optional: Uncomment to enable GPIO wakeup pin control
         * wakeup-gpios = <&gpio0 15 GPIO_ACTIVE_HIGH>;
         *
         * Use argos_smd_wakeup_enable() to set HIGH before communicating
         * Use argos_smd_wakeup_disable() to set LOW when done
         */
    };
};

zephyr_udc0: &usbd {
	compatible = "nordic,nrf-usbd";
	status = "okay";
	cdc_acm_uart: cdc_acm_uart0 {
		compatible = "zephyr,cdc-acm-uart";
		label = "CDC_ACM";
	};
};
